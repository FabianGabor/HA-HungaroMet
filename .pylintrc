[MASTER]
ignore=tests

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-many-instance-attributes,
    too-few-public-methods,
    no-member,
    no-name-in-module,
    import-error,
    attribute-defined-outside-init,
    unused-argument,
    consider-using-f-string,
    broad-except,
    duplicate-code,
    too-many-arguments,
    too-many-positional-arguments,
    overridden-final-method,
    protected-access,
    too-many-locals,
    too-many-branches,
    too-many-statements
